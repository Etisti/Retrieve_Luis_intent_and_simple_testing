.PHONY: build help

# ================================================================================================
# Settings
# ================================================================================================

REPO = Environnement_Retrieve_Luis_intent_and_simple_testing 


# ================================================================================================
# Commands
# ================================================================================================

build: 
	docker build --tag ${REPO} .

run: 
	docker run --rm -ti ${REPO} 

help:
	@echo "This is a makefile to build the app from all environement"